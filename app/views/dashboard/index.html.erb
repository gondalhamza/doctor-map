<h1>Doctor's Dashboard</h1>
<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

<div style='width: 100%;'>
  <div id="map" style='width: 190vh; height: 90vh;'></div>
</div>

<script type="text/javascript">  
	let map;
	
	
	var mapCenter = new google.maps.LatLng(52.5167, 13.3833);

	initMap = function() {
			var locations = [
	        ['Marker 1', 48.0, 12.4],
	        ['Marker 2', 47.0, 15.4],
	        ['Marker 3', 45.0, 15.4],
	        ['Marker 4', 44.0, 15.4],
	        ['Marker 5', 46.0, 15.4],
	    ];

    var center, marker;
    center = { lat: 45.0, lng: 11.644 };
    map = new google.maps.Map(document.getElementById('map'), {
      zoom: 8,
      center: center
    });
    marker = new google.maps.Marker({
      position: center,
      map: map
    });

    for (i = 0; i < locations.length; i++) {
      marker = new google.maps.Marker({
          position: new google.maps.LatLng(locations[i][1], locations[i][2]),
          map: map
      });
    }
  };
</script>

<script defer
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBND3WVtTVm3wC25rQwbd0KPbPttt9W3m0&callback=initMap">
</script>